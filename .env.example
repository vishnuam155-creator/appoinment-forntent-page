# API Configuration
# Copy this file to .env.local and update with your actual backend URL

# Backend API Base URL
# For local development, this might be http://localhost:8000
# For production, use your actual backend URL
VITE_API_BASE_URL=http://localhost:8000

# Optional: API timeout in milliseconds (default: 30000)
# VITE_API_TIMEOUT=30000

# Optional: Enable debug mode for API calls
# VITE_API_DEBUG=false
